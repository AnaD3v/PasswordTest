{
  "name": "passwordtesting",
  "version": "1.0.0",
  "description": "Aplicativo de teste de senhas para automação de logins",
  "author": "Ana Flávia",
  "type": "module",
  "main": "./src/main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "electron-log": "^5.3.1",
    "electron-updater": "^6.3.9",
    "express": "^4.21.2",
    "puppeteer": "^24.3.1"
  },
  "devDependencies": {
    "electron": "^35.0.0",
    "electron-builder": "^25.1.8"
  },
  "build": {
    "appId": "com.seuapp.passwordtesting",
    "productName": "PasswordTesting",
    "asar": false,
    "asarUnpack": "**/*.{exe,dll,bin,so,dmg}",
    "win": {
      "target": "nsis",
      "icon": "./public/icons/app-icon.ico",
      "signtoolOptions": {
        "signingHashAlgorithms": []
      },
      "publish": [
        {
          "provider": "github",
          "owner": "AnaD3v",
          "repo": "TestPassword"
        }
      ]
    },
    "mac": {
      "target": "dmg",
      "icon": "./public/icons/app-icon.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "./public/icons/app-icon.png"
    }
  }
}
